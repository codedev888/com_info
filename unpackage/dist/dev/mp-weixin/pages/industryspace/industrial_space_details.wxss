.group-title {
  font-size: 30rpx;
  color: #999999;
}
.group-title .changeBar {
  width: 200rpx;
  height: 60rpx;
  border: #627bf8 solid 2rpx;
  border-radius: 30rpx;
  position: relative;
}
.group-title .changeBar text {
  width: 50%;
  display: block;
  text-align: center;
  line-height: 58rpx;
  border-radius: 30rpx;
  position: relative;
  z-index: 2;
}
.group-title .changeBar:before {
  content: "";
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  display: block;
  height: 58rpx;
  border-radius: 30rpx;
  width: 50%;
  background: #627bf8;
  -webkit-transition: all 0.5s;
  transition: all 0.5s;
}
.group-title .changeBar.left:before {
  left: 50%;
}
.group-title .changeBar text.active {
  /* background:rgba(98, 123, 248, 1); */
  color: #ffffff;
}
.set-bar {
  position: fixed;
  width: 100%;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  bottom: 0rpx;
  left: 0rpx;
  background: #ffffff;
}
.section3 {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  line-height: 100rpx;
  text-align: center;
  font-size: 28rpx;
  color: #6482ff;
  border-left: #f2f2f2 solid 2rpx;
}
.section3.active {
  background-color: #6482ff;
  color: #ffffff;
}
.section3:after {
  border: 0;
  border-radius: 0rpx;
}
.section3:first {
  border: 0;
}
uni-button {
  border-radius: 0;
}

